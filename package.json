{
  "name": "medicine-assistant",
  "version": "1.1.0",
  "description": "Medicine Assistant Backend (NestJS + PostgreSQL)",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
  "prebuild": "rimraf build",
  "build": "tsc -p tsconfig.build.json",
  "build:all": "npm run build && npm run generate:openapi",
    "start": "node build/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node build/main.js",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "ts-node -r dotenv/config -r tsconfig-paths/register src/db/run-migrations.ts",
    "db:revert": "ts-node -r dotenv/config -r tsconfig-paths/register src/db/revert-migration.ts",
    "db:seed": "ts-node -r dotenv/config -r tsconfig-paths/register src/db/seed.ts",
    "db:create": "ts-node -r dotenv/config -r tsconfig-paths/register scripts/create-database.ts",
    "seed": "npm run db:seed",
  "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
  "db:reset": "npm run db:revert && npm run db:migrate && npm run db:seed",
  "generate:openapi": "ts-node -P tsconfig.openapi.json src/openapi/generate-spec.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "test/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.8.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^3.0.12",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.5",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
